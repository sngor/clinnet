# Clinnet EMR Frontend Environment Configuration
# Copy this file to .env.local for development or .env.production for production builds

# API Configuration (Vite format - preferred)
VITE_API_ENDPOINT=https://your-api-gateway-url.execute-api.region.amazonaws.com/dev
VITE_ENVIRONMENT=dev

# API Configuration (React format - for deployment compatibility)
REACT_APP_API_URL=https://your-api-gateway-url.execute-api.region.amazonaws.com/dev
REACT_APP_ENVIRONMENT=dev

# AWS Cognito Configuration (Vite format)
VITE_USER_POOL_ID=us-east-1_xxxxxxxxx
VITE_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
VITE_COGNITO_REGION=us-east-1

# AWS Cognito Configuration (React format - for deployment compatibility)
REACT_APP_USER_POOL_ID=us-east-1_xxxxxxxxx
REACT_APP_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_AWS_REGION=us-east-1

# S3 Configuration
VITE_S3_BUCKET=clinnet-documents-bucket-name
VITE_S3_REGION=us-east-1
REACT_APP_S3_BUCKET=clinnet-documents-bucket-name
REACT_APP_S3_REGION=us-east-1

# Application Configuration
VITE_APP_NAME=Clinnet EMR
VITE_APP_VERSION=1.0.0
REACT_APP_APP_NAME=Clinnet EMR
REACT_APP_VERSION=1.0.0

# Build Configuration
GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true

# Optional: Analytics and Monitoring
# VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# VITE_SENTRY_DSN=https://xxxxxxxxx@sentry.io/xxxxxxx
# REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# REACT_APP_SENTRY_DSN=https://xxxxxxxxx@sentry.io/xxxxxxx